
scale = 18
n = 4
x = 0.4

for (k = 0; k <= n; k++) {
    u[k] = x

    r[k] = rec(r[], u[])

    e[k] = exp(e[], u[])

    s[k] = sin(c[], u[])
    c[k] = cos(s[], u[])
    t[k] = tan(s2[], u[])
    s2[k] = sec2(t[])

    sh[k] = sinh(ch[], u[])
    ch[k] = cosh(sh[], u[])
    th[k] = tanh(sh2[], u[])
    sh2[k] = sech2(th[])
}

print "\nu * 1/u\n"
for (k = 0; k <= n; k++) {
    p[k] = mul(u[], r[])
    print k, " ", u[k], " ", r[k], " ", p[k], "\n"
}

print "\nu*u / u^2\n"
for (k = 0; k <= n; k++) {
    v[k] = mul(u[], u[])
    p[k] = pwr(p[], u[], 2)
    d[k] = div(d[], v[], p[])
    print k, " ", u[k], " ", v[k], " ", p[k], " ", d[k], "\n"
}

print "\nrec-pwr\n"
for (k = 0; k <= n; k++) {
    p[k] = pwr(p[], u[], -1)
    print k, " ", u[k], " ", r[k], " ", p[k], "\n"
}

print "\nexp-log\n"
for (k = 0; k <= n; k++) {
    l[k] = ln(l[], e[])
    print k, " ", u[k], " ", e[k], " ", l[k], "\n"
}

print "\nrec-log\n"
for (k = 0; k <= n; k++) {
    l1[k] = ln(l1[], u[])
    l2[k] = ln(l2[], r[])
    print k, " ", u[k], " ", l1[k], " ", - l2[k], "\n"
}

print "\npwr-log--2\n"
for (k = 0; k <= n; k++) {
    p[k] = pwr(p[], u[], -2)
    l1[k] = ln(l1[], p[])
    l2[k] = ln(l2[], u[])
    print k, " ", u[k], " ", l1[k], " ", - 2 * l2[k], "\n"
}

print "\npwr-log-2\n"
for (k = 0; k <= n; k++) {
    p[k] = pwr(p[], u[], 2)
    l1[k] = ln(l1[], p[])
    l2[k] = ln(l2[], u[])
    print k, " ", u[k], " ", l1[k], " ", 2 * l2[k], "\n"
}

print "\nsin-asin\n"
for (k = 0; k <= n; k++) {
    as[k] = asin(as[], c[], s[])
    print k, " ", u[k], " ", s[k], " ", as[k], "\n"
}

print "\ncos-acos\n"
for (k = 0; k <= n; k++) {
    ac[k] = acos(ac[], s[], c[])
    print k, " ", u[k], " ", c[k], " ", ac[k], "\n"
}

print "\ntan-atan\n"
for (k = 0; k <= n; k++) {
    at[k] = atan(at[], s2[], t[])
    print k, " ", u[k], " ", t[k], " ", at[k], "\n"
}

print "\nsinh-asinh\n"
for (k = 0; k <= n; k++) {
    ash[k] = asinh(ash[], ch[], sh[])
    print k, " ", u[k], " ", sh[k], " ", ash[k], "\n"
}

print "\ncosh-acosh\n"
for (k = 0; k <= n; k++) {
    ach[k] = acosh(ach[], sh[], ch[])
    print k, " ", u[k], " ", ch[k], " ", ach[k], "\n"
}

print "\ntanh-atanh\n"
for (k = 0; k <= n; k++) {
    ath[k] = atanh(ath[], sh2[], th[])
    print k, " ", u[k], " ", th[k], " ", ath[k], "\n"
}

print "\npwr-pwr\n"
for (k = 0; k <= n; k++) {
    p1[k] = pwr(p1[], u[], 4)
    p2[k] = pwr(p2[], p1[], 0.25)
    print k, " ", u[k], " ", p1[k], " ", p2[k], "\n"
}

print "\ncomposition\n"
for (k = 0; k <= n; k++) {
    ash[k] = asinh(ash[], tmpa[], t[])
    tmpa[k] = cosh(t[], ash[])

    ath[k] = atanh(ath[], tmpb[], s[])
    tmpb[k] = sech2(s[])

    tmp1[k] = const(1) + s[k]
    tmp2[k] = div(tmp2[], tmp1[], c[])
    tmp3[k] = ln(tmp3[], tmp2[])

    print k, " ", u[k], " ", ash[k], " ", ath[k], " ", tmp3[k], "\n"
}

quit
