            --field="x0" --field="y0" --field="z0" --field="a" --field="b" \
            --  '32!2..64!1' '1.0' './tsm-genesio-tesi-std' \
                '6!0..36!3' '53!63!113!^237' '_' '.01!0.001..0.1!0.01!3' '50000!1..1000000!1000' \
                '0.1' '0.1' '0.1' '0.446' '1.1') ;;
    esac ;;
    ISUC) case $analysis in
        Gnuplot) ./plot ./tsm-isuc-std $(yad --columns=2 --title="ISUC" --form --separator=" " --align=right \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                '0.05' '-0.05' '0.3' '0.1' '0.1' '4.2') ;;
        CNS) ./cns $(yad --columns=2 --title="ISUC" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  'step2!nosim' '1.0' './tsm-isuc-std' \
                '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                '0.05' '-0.05' '0.3' '0.1' '0.1' '4.2') ;;
        CNSScan) ./cns-scan $(yad --columns=2 --title="ISUC" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  '32!2..64!1' '1.0' './tsm-isuc-std' \
                '6!0..36!3' '53!63!113!^237' '_' '.01!0.001..0.1!0.01!3' '60000!1..1000000!1000' \
                '0.05' '-0.05' '0.3' '0.1' '0.1' '4.2') ;;
    esac ;;
esac
