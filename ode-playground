#!/bin/sh
#
#  (c) 2018-2023 m4r35n357@gmail.com (Ian Smith), for licencing see the LICENCE file

. ./base.sh

choice=$(yad --columns=2 --title="ODE Playground" --form --separator=" " --align=right \
    --field="Model:CB" --field="Analysis type:CB" \
    -- 'Bouali!BurkeShaw!GenesioTesi!Halvorsen!ISUC!^Lorenz!RF!Rossler!Rucklidge!Thomas!WimolBanlue!YuWang' \
       'GLplot!^Gnuplot!CNS!CNSScan!ChaosScan')

case $? in
    1) exit;;
esac

set $choice
model=$1
analysis=$2

trail_length='1000!1000..10000!1000'
places='15!3..36!3'
order='4!2..32!2'
step_size='.01!0.001..0.1!0.001!3'
steps='^10000!20000!50000!100000!200000!500000!1000000'
cns_range='16!2..32!1'
cns_mode='^step2!nosim'
cns_separation='0.1!^1.0!10.0'

case $model in
    Lorenz) case $analysis in
        GLplot) $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  './tsm-lorenz-gl' $trail_length $order $step_size $steps \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  './tsm-lorenz-std' $places $order $step_size $steps \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  $cns_mode $cns_separation \
                './tsm-lorenz-std' $places $order $step_size $steps \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
        CNSScan) time -p ./cns-scan $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  $cns_range $cns_separation \
                './tsm-lorenz-std' $places '_' $step_size $steps \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
        ChaosScan) time -p ./chaos-scan $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Lower Value" --field="Upper Value" --field="Samples":CB --field="Snapshots":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho":RO --field="beta(n)" --field="beta(d)" \
            --  '0.0' '50.0' $steps 'no-snaps!snaps' \
                './tsm-lorenz-std' $places $order $step_size $steps \
                '-15.8' '-18.48' '35.64' '10' '$p' '8' '3') ;;
    esac ;;
    Rossler) case $analysis in
        GLplot) $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  './tsm-rossler-gl' $trail_length $order $step_size $steps \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  './tsm-rossler-std' $places $order $step_size $steps \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  $cns_mode $cns_separation \
                './tsm-rossler-std' $places $order $step_size $steps \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
        CNSScan) time -p ./cns-scan $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  $cns_range $cns_separation \
                './tsm-rossler-std' $places '_' $step_size $steps \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
        ChaosScan) time -p ./chaos-scan $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Lower Value" --field="Upper Value" --field="Samples":CB --field="Snapshots":CB \
	        --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c":RO \
            --  '0.0' '50.0' $steps 'no-snaps!snaps' \
                './tsm-rossler-std' $places $order $step_size $steps \
                '0.0' '-6.78' '0.02' '.2' '.2' '$p') ;;
    esac ;;
    Thomas) case $analysis in
        GLplot) $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  './tsm-thomas-gl' $trail_length $order $step_size $steps \
                '1.0' '0.0' '0.0' '0.185') ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  './tsm-thomas-std' $places $order $step_size $steps \
                '1.0' '0.0' '0.0' '0.185') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  $cns_mode $cns_separation \
                './tsm-thomas-std' $places $order $step_size $steps \
                '1.0' '0.0' '0.0' '0.185') ;;
        CNSScan) time -p ./cns-scan $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  $cns_range $cns_separation \
                './tsm-thomas-std' $places '_' $step_size $steps \
                '1.0' '0.0' '0.0' '0.185') ;;
        ChaosScan) time -p ./chaos-scan $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Lower Value" --field="Upper Value" --field="Samples":CB --field="Snapshots":CB \
	        --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="b":RO \
            --  '0.1' '0.23' $steps 'no-snaps!snaps' \
                './tsm-thomas-std' $places $order $step_size $steps \
                '1.0' '0.0' '0.0' '$p') ;;
    esac ;;
    RF) case $analysis in
        GLplot) $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  './tsm-rf-gl' $trail_length $order $step_size $steps \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  './tsm-rf-std' $places $order $step_size $steps \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  $cns_mode $cns_separation \
                './tsm-rf-std' $places $order $step_size $steps \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
        CNSScan) time -p ./cns-scan $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  $cns_range $cns_separation \
                './tsm-rf-std' $places '_' $step_size $steps \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
        ChaosScan) time -p ./chaos-scan $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Lower Value" --field="Upper Value" --field="Samples":CB --field="Snapshots":CB \
	        --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="alpha":RO --field="gamma" \
            --  '0.1' '0.35' $steps 'no-snaps!snaps' \
                './tsm-rf-std' $places $order $step_size $steps \
                '0.05' '-0.05' '0.3' '$p' '.1') ;;
    esac ;;
    Halvorsen) case $analysis in
        GLplot) $(yad --columns=2 --title="Halvorsen" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" \
            --  './tsm-halvorsen-gl' $trail_length $order $step_size $steps \
                '1.0' '0.0' '0.0' '1.4') ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="Halvorsen" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" \
            --  './tsm-halvorsen-std' $places $order $step_size $steps \
                '1.0' '0.0' '0.0' '1.4') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Halvorsen" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" \
            --  $cns_mode $cns_separation \
                './tsm-halvorsen-std' $places $order $step_size $steps \
                '1.0' '0.0' '0.0' '1.4') ;;
        CNSScan) time -p ./cns-scan $(yad --columns=2 --title="Halvorsen" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" \
            --  $cns_range $cns_separation \
                './tsm-halvorsen-std' $places '_' $step_size $steps \
                '1.0' '0.0' '0.0' '1.4') ;;
        ChaosScan) time -p ./chaos-scan $(yad --columns=2 --title="Halvorsen" --form --separator=" " --align=right \
            --field="Lower Value" --field="Upper Value" --field="Samples":CB --field="Snapshots":CB \
	        --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a":RO \
            --  '1' '4' $steps 'no-snaps!snaps' \
                './tsm-halvorsen-std' $places $order $step_size $steps \
                '1.0' '0.0' '0.0' '$p') ;;
    esac ;;
    WimolBanlue) case $analysis in
        GLplot) $(yad --columns=2 --title="Wimol-Banlue" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" \
            --  './tsm-wimol-banlue-gl' $trail_length $order $step_size $steps \
