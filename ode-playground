            --field="Maximum Order":NUM --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  '32!2..64!1' '1.0' './tsm-rf-std' \
                '6!0..36!3' '53!63!113!^237' '_' '0.01!0.001..0.1!0.001!3' '100000!1..1000000!1000' \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
    esac ;;
    Halvorsen) case $analysis in
        Gnuplot) ./plot ./tsm-halvorsen-std $(yad --columns=2 --title="Halvorsen" --form --separator=" " --align=right \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" \
            --  '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
