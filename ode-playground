#!/bin/sh
#
#  (c) 2018-2023 m4r35n357@gmail.com (Ian Smith), for licencing see the LICENCE file

user_dir="/tmp/$USER"
[ ! -d $user_dir ] && mkdir $user_dir
user_data="$user_dir/data"

choice=$(yad --columns=2 --title="ODE Playground" --form --separator=" " --align=right \
    --field="Model:CB" --field="Analysis type:CB" \
    -- 'Bouali!BurkeShaw!GenesioTesi!Halvorsen!ISUC!Lorenz!RF!Rossler!Rucklidge!^Thomas!WimolBanlue!YuWang' \
       '^GLplot!Gnuplot!CNS!CNSscan!Bifurcation')

case $? in
    1) exit;;
esac

set $choice
model=$1
analysis=$2

plot () {
 gnuplot -p << EOF
set terminal wxt background rgb "grey85"
set xyplane 0
set view 54.73561,135
set xlabel 'X'
set ylabel 'Y'
set zlabel 'Z'
splot '$user_data' with lines
EOF
}

case $model in
    Lorenz)
        case $analysis in
            GLplot) ./tsm-lorenz-gl \
                $(yad --columns=2 --title="Lorenz OpenGL" --form --separator=" " --align=right \
                --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
                --  '5000!1000..10000!1000' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
            Gnuplot) ./tsm-lorenz-std \
                $(yad --columns=2 --title="Lorenz Gnuplot" --form --separator=" " --align=right \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
                --  '6!0..36!3' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '-15.8' '-18.48' '35.64' '10' '28' '8' '3') >$user_data; plot ;;
            CNS) ./cns \
                $(yad --columns=2 --title="Lorenz CNS" --form --separator=" " --align=right \
                --field="Mode":CB --field="Separation" --field="Model":RO \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
                --  'step2!nosim' '1.0' './tsm-lorenz-std' \
                    '6!0..36!3' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
            CNSscan) ./cns-scan \
                $(yad --columns=2 --title="Lorenz CNS Scan" --form --separator=" " --align=right \
                --field="Maxium Order":NUM --field="Separation" --field="Model":RO \
                --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
                --  '32!2..32!1' '1.0' './tsm-lorenz-std' \
                    '6!0..36!3' '_' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
            Bifurcation) time -p ./bifurcation-scan \
                $(yad --columns=2 --title="Lorenz Bifurcation" --form --separator=" " --align=right \
                --field="Lower Value" --field="Upper Value" --field="Skip First 1/":CB --field="Model":RO \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho":RO --field="beta(n)" --field="beta(d)" \
                --  '0.0' '50.0' '10!2' './tsm-lorenz-std' \
                    '6!0..36!3' '4!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '-15.8' '-18.48' '35.64' '10' '$p' '8' '3') ;;
        esac ;;
    Rossler)
        case $analysis in
            GLplot) ./tsm-rossler-gl \
                $(yad --columns=2 --title="Rossler OpenGL" --form --separator=" " --align=right \
                --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
                --  '5000!1000..10000!1000' '8!4..32!1' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                    '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
            Gnuplot) ./tsm-rossler-std \
                $(yad --columns=2 --title="Rossler Gnuplot" --form --separator=" " --align=right \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
                --  '6!0..36!3' '8!4..32!1' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                    '0.0' '-6.78' '0.02' '.2' '.2' '5.7') >$user_data; plot ;;
            CNS) ./cns \
                $(yad --columns=2 --title="Rossler CNS" --form --separator=" " --align=right \
                --field="Mode":CB --field="Separation" --field="Model":RO \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
                --  'step2!nosim' '1.0' './tsm-rossler-std' \
                    '6!0..36!3' '8!4..32!1' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                    '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
            CNSscan) ./cns-scan \
                $(yad --columns=2 --title="Rossler CNS Scan" --form --separator=" " --align=right \
                --field="Maxium Order":NUM --field="Separation" --field="Model":RO \
                --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
                --  '32!2..32!1' '1.0' './tsm-rossler-std' \
                    '6!0..36!3' '_' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                    '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
            Bifurcation) time -p ./bifurcation-scan \
                $(yad --columns=2 --title="Rossler Bifurcation" --form --separator=" " --align=right \
                --field="Lower Value" --field="Upper Value" --field="Skip First 1/":CB --field="Model":RO \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c":RO \
                --  '0.0' '50.0' '10!2' './tsm-rossler-std' \
                    '6!0..36!3' '4!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '0.0' '-6.78' '0.02' '.2' '.2' '$p') ;;
        esac ;;
    Thomas)
        case $analysis in
            GLplot) ./tsm-thomas-gl \
                $(yad --columns=2 --title="Thomas OpenGL" --form --separator=" " --align=right \
                --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="b" \
                --  '5000!1000..10000!1000' '8!4..32!1' '.1!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '1.0' '0.0' '0.0' '0.185') ;;
            Gnuplot) ./tsm-thomas-std \
                $(yad --columns=2 --title="Thomas Gnuplot" --form --separator=" " --align=right \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="b" \
                --  '6!0..36!3' '8!4..32!1' '.1!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '1.0' '0.0' '0.0' '0.185') >$user_data; plot ;;
            CNS) ./cns \
                $(yad --columns=2 --title="Thomas CNS" --form --separator=" " --align=right \
                --field="Mode":CB --field="Separation" --field="Model":RO \
                --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="b" \
                --  'step2!nosim' '1.0' './tsm-thomas-std' \
                    '6!0..36!3' '8!4..32!1' '.1!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '1.0' '0.0' '0.0' '0.185') ;;
            CNSscan) ./cns-scan \
                $(yad --columns=2 --title="Thomas CNS Scan" --form --separator=" " --align=right \
                --field="Maxium Order":NUM --field="Separation" --field="Model":RO \
                --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
                --field="x0" --field="y0" --field="z0" --field="b" \
                --  '32!2..64!1' '1.0' './tsm-thomas-std' \
                    '6!0..36!3' '_' '.1!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                    '1.0' '0.0' '0.0' '0.185') ;;
            Bifurcation) time -p ./bifurcation-scan \
                $(yad --columns=2 --title="Thomas Bifurcation" --form --separator=" " --align=right \
                --field="Lower Value" --field="Upper Value" --field="Skip First 1/":CB --field="Model":RO \
