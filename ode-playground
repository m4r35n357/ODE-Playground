            --  './tsm-wimol-banlue-std' $places $order $step_size $steps \
                '1.0' '0.1' '0.1' '1.0') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Wimol-Banlue" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" \
