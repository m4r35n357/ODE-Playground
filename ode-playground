    YuWang) case $analysis in
        GLplot) ./tsm-yu-wang-gl $(yad --columns=2 --title="Yu-Wang" --form --separator=" " --align=right \
            --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
