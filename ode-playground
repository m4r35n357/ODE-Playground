	    --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c":RO \
            --  '0.0' '50.0' $steps 'no-snaps!snaps' \
                './tsm-isuc-std' $places $order $step_size $steps \
                '0.05' '-0.05' '0.3' '0.1' '0.1' '$p') ;;
    esac ;;
esac
