    BurkeShaw) case $analysis in
        GLplot) $(yad --columns=2 --title="Burke-Shaw" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="S" --field="V" \
            --  './tsm-burke-shaw-gl' $trail_length $order $step_size $steps \
                '1.0' '1.0' '1.0' '10.0' '4.272') ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="Burke-Shaw" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="S" --field="V" \
            --  './tsm-burke-shaw-std' $places $order $step_size $steps \
                '1.0' '1.0' '1.0' '10.0' '4.272') ;;
        CNS) time -p ./cns $(yad --columns=2 --title="Burke-Shaw" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" \
            --  $cns_mode $cns_separation \
                './tsm-burke-shaw-std' $places $order $step_size $steps \
                '1.0' '1.0' '1.0' '10.0' '4.272') ;;
        CNSScan) time -p ./cns-scan $(yad --columns=2 --title="Burke-Shaw" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation":CB \
            --field="Model":RO --field="Display Places":NUM --field="Order":RO --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" \
            --  $cns_range $cns_separation \
                './tsm-burke-shaw-std' $places '_' $step_size $steps \
                '1.0' '1.0' '1.0' '10.0' '4.272') ;;
        ChaosScan) time -p ./chaos-scan $(yad --columns=2 --title="Burke-Shaw" --form --separator=" " --align=right \
            --field="Lower Value" --field="Upper Value" --field="Samples":CB --field="Snapshots":CB \
	        --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="S" --field="V":RO \
            --  '0.0' '100.0' $steps 'no-snaps!snaps' \
                './tsm-burke-shaw-std' $places $order $step_size $steps \
                '1.0' '1.0' '1.0' '10.0' '$p') ;;
    esac ;;
    Rucklidge) case $analysis in
        GLplot) $(yad --columns=2 --title="Rucklidge" --form --separator=" " --align=right \
