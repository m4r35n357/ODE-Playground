#!/bin/sh
#
#  (c) 2018-2023 m4r35n357@gmail.com (Ian Smith), for licencing see the LICENCE file

user_dir="/tmp/$USER"
[ ! -d $user_dir ] && mkdir $user_dir
user_data="$user_dir/data"

choice=$(yad --columns=2 --title="ODE Playground" --form --separator=" " --align=right \
    --field="Model:CB" --field="Analysis type:CB" \
    -- 'Bouali!BurkeShaw!GenesioTesi!Halvorsen!ISUC!Lorenz!RF!Rossler!Rucklidge!^Thomas!WimolBanlue!YuWang' \
       '^Gnuplot!CNS!CNSScan')

case $? in
    1) exit;;
esac

set $choice
model=$1
analysis=$2

case $model in
    Lorenz) case $analysis in
        Gnuplot) ./plot ./tsm-lorenz-std $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
        CNS) ./cns $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  'step2!nosim' '1.0' './tsm-lorenz-std' \
                '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
        CNSScan) ./cns-scan $(yad --columns=2 --title="Lorenz" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="sigma" --field="rho" --field="beta(n)" --field="beta(d)" \
            --  '32!2..32!1' '1.0' './tsm-lorenz-std' \
                '6!0..36!3' '53!63!113!^237' '_' '.01!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                '-15.8' '-18.48' '35.64' '10' '28' '8' '3') ;;
    esac ;;
    Rossler) case $analysis in
        Gnuplot) ./plot ./tsm-rossler-std $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
        CNS) ./cns $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  'step2!nosim' '1.0' './tsm-rossler-std' \
                '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
        CNSScan) ./cns-scan $(yad --columns=2 --title="Rossler" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  '32!2..32!1' '1.0' './tsm-rossler-std' \
                '6!0..36!3' '53!63!113!^237' '_' '.01!0.001..0.1!0.001!3' '60000!1..1000000!1000' \
                '0.0' '-6.78' '0.02' '.2' '.2' '5.7') ;;
    esac ;;
    Thomas) case $analysis in
        Gnuplot) ./plot ./tsm-thomas-std $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.1!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                '1.0' '0.0' '0.0' '0.185') ;;
        CNS) ./cns $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  'step2!nosim' '1.0' './tsm-thomas-std' \
                '6!0..36!3' '53!63!113!^237' '8!4..32!1' '.1!0.001..0.1!0.001!3' '10000!1..1000000!1000' \
                '1.0' '0.0' '0.0' '0.185') ;;
        CNSScan) ./cns-scan $(yad --columns=2 --title="Thomas" --form --separator=" " --align=right \
            --field="Maximum Order":NUM --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":RO --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="b" \
            --  '32!2..64!1' '1.0' './tsm-thomas-std' \
                '6!0..36!3' '53!63!113!^237' '_' '.1!0.001..0.1!0.001!3' '25000!1..1000000!1000' \
                '1.0' '0.0' '0.0' '0.185') ;;
    esac ;;
    RF) case $analysis in
        Gnuplot) ./plot ./tsm-rf-std $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  '6!0..36!3' '53!63!113!^237' '8!4..32!1' '0.01!0.001..0.1!0.001!3' '100000!1..1000000!1000' \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
        CNS) ./cns $(yad --columns=2 --title="Rabinovich–Fabrikant" --form --separator=" " --align=right \
            --field="Mode":CB --field="Separation" --field="Model":RO \
            --field="Display Places":NUM --field="Precision (bits)":CB --field="Order":NUM --field="Step Size":NUM --field="Steps":NUM \
            --field="x0" --field="y0" --field="z0" --field="alpha" --field="gamma" \
            --  'step2!nosim' '1.0' './tsm-rf-std' \
                '6!0..36!3' '53!63!113!^237' '8!4..32!1' '0.01!0.001..0.1!0.001!3' '100000!1..1000000!1000' \
                '0.05' '-0.05' '0.3' '.2875' '.1') ;;
