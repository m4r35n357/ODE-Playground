                './tsm-genesio-tesi-std' $places $order $step_size $steps \
                '0.1' '0.1' '0.1' '$p' '1.1') ;;
    esac ;;
    ISUC) case $analysis in
        GLplot) $(yad --columns=2 --title="ISUC" --form --separator=" " --align=right \
            --field="Model":RO --field="Trail Length":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
            --field="x0" --field="y0" --field="z0" --field="a" --field="b" --field="c" \
            --  './tsm-isuc-gl' $trail_length $order $step_size $steps \
                '0.05' '-0.05' '0.3' '0.1' '0.1' '4.2' ) ;;
        Gnuplot) ./plot 10000 $(yad --columns=2 --title="ISUC" --form --separator=" " --align=right \
            --field="Model":RO --field="Display Places":NUM --field="Order":NUM --field="Step Size":NUM --field="Steps":CB \
